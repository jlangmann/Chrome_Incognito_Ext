{
	"name": "Open In Incognito",
	"version": "1.0",
	"description": "Open current page in Incognito",
	"permissions":["activeTab"],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_title": "Do something"
	},
	"icons": {
      "16": "images/incognito-16.png",
      "48": "images/incognito-48.png",
      "128": "images/incognito-128.png"
    },
	"manifest_version": 2
}
